{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/signs/*",
      "/api/*",
      "/auth/*",
      "/files/*",
      "/field/*",
      "/field-upload-xml/*",
      "/run-bes/*",
      "/releases/*",
      "/oil-field-operations/*",
      "/app-field/*",
      "/privacy/*"
    ]
  },
  "routes": [
    {
      "route": "/signs/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/auth/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/files/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/field/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/field-upload-xml/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/run-bes/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/releases/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/oil-field-operations/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/app-field/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/privacy/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}